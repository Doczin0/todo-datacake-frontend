{
  "name": "todo-mobile",
  "version": "1.0.0",
  "private": true,
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "npx expo start",
    "android": "npx expo start --android",
    "ios": "npx expo start --ios",
    "web": "npx expo start --web",
    "start:web": "cross-env CI=1 npx expo start --web --port 19006",
    "start:docker": "concurrently -k -s first -n backend,frontend \"cd .. && gunicorn server.wsgi:application --chdir backend --bind 0.0.0.0:8000\" \"cross-env CI=1 npx expo start --web --port 19006 --clear\"",
    "lint": "npx expo lint"
  },
  "dependencies": {
    "@expo/metro-runtime": "~3.2.3",
    "@expo/vector-icons": "^14.0.2",
    "@react-native-community/datetimepicker": "^7.2.0",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.10.1",
    "@tanstack/react-query": "^5.51.0",
    "axios": "^1.7.7",
    "date-fns": "^3.6.0",
    "expo": "~51.0.5",
    "expo-av": "~14.0.2",
    "expo-font": "12.0.10",
    "expo-random": "~14.0.1",
    "expo-status-bar": "~1.12.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.74.3",
    "react-native-confetti-cannon": "1.5.2",
    "react-native-gesture-handler": "~2.16.2",
    "react-native-modal-datetime-picker": "^15.0.0",
    "react-native-safe-area-context": "4.9.0",
    "react-native-screens": "~3.30.1",
    "react-native-web": "~0.19.10"
  },
  "devDependencies": {
    "@babel/core": "^7.24.9",
    "@expo/cli": "^0.17.11",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-expo": "~7.1.2"
  },
  "engines": {
    "node": ">=18"
  }
}
